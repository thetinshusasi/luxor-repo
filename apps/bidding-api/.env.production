# This should not be committed in Real repo. But commiting here for assignement purposes to 
# to make the project assessment easy  and there are no real secrets here

# Production environment configuration for bidding-api
NODE_ENV=production
PORT=3001
API_PREFIX=api

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=luxor_bidding_prod
DB_USER=your-production-user
DB_PASSWORD=your-production-password

# JWT configuration
JWT_SECRET=your-production-jwt-secret-key
JWT_EXPIRES_IN=100000s
CACHE_TTL=3600

SYNCHRONIZE=false

# CORS configuration
CORS_ORIGIN=https://your-production-domain.com

# Logging
LOG_LEVEL=info

# Production specific settings
DEBUG=false
ENABLE_SWAGGER=false
ENABLE_CORS=true
